{
  "name": "Focus Blocker",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Block certain websites during designated focus times.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["webNavigation", "storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  }
}
